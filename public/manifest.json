{
  "manifest_version": 2,
  "name": "switchHost",
  "version": "0.1",
  "description": "代理配置扩展",
  "browser_action": {
    "default_icon": "icons/icon_32.png",
    "default_title": "switchHost",
    "default_popup": "popup.html"
  },
  "options_page": "setting.html",
  "content_scripts": [],
  "background": {
    "scripts": [],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "128": "icons/icon_128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "打开popup弹窗的快捷键"
    }
  },
  "permissions": ["storage"]
}
